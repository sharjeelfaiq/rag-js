{
  "name": "backend",
  "version": "1.0.0",
  "type": "module",
  "license": "ISC",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "format": "prettier --write \"./**/*.{js,jsx,html,yaml,env,md,json,txt}\"",
    "lint:js": "eslint \"./dist/**/*.js\"",
    "check circularDeps": "npx madge --circular ./src",
    "seed": "node src/scripts/seed.js",
    "generateReadme": "readmeai --repository . --badge-style FLAT-SQUARE --badge-color E92063 --header-style COMPACT --navigation-style ACCORDION --emojis RAINBOW --logo ICE"
  },
  "devDependencies": {
    "eslint": "^9.20.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.0"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.41.3",
    "colors": "^1.4.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "envalid": "^8.1.0",
    "express": "^4.21.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "http-errors": "^2.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.9.6",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "multer-storage-cloudinary": "^4.0.0",
    "nodemailer": "^6.10.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xss-clean": "^0.1.4"
  },
  "jest": {
    "testEnvironment": "node",
    "transform": {}
  },
  "imports": {
    "#config/*": "./src/config/*",
    "#constants/*": "./src/constants/*",
    "#repository/*": "./src/repository/*",
    "#middleware/*": "./src/middleware/*",
    "#models/*": "./src/models/*",
    "#modules/*": "./src/modules/*",
    "#routes/*": "./src/routes/*",
    "#server/*": "./src/server/*",
    "#utils/*": "./src/utils/*"
  }
}
